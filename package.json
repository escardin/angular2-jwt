{
  "name": "angular2-jwt",
  "version": "0.1.27",
  "description": "Helper library for handling JWTs in Angular 2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/angular2-jwt.git"
  },
  "scripts": {
    "clean":"rimraf dist/",
    "dev": "tsc --watch",
    "test": "karma start",
    "build:jit": "tsc",
    "build:aot": "ngc",
    "build":"npm run clean && npm run build:jit && npm run build:aot",
    "prepublish":"npm run build && npm run test"
  },
  "keywords": [
    "angular",
    "angular2",
    "jwt",
    "authentication"
  ],
  "author": "ryanchenkie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/angular2-jwt/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/auth0/angular2-jwt#readme",
  "devDependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/compiler-cli": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/platform-browser": "^2.4.2",
    "@types/jasmine": "^2.2.33",
    "@types/js-base64": "^2.1.3",
    "awesome-typescript-loader": "^2.2.4",
    "core-js": "^2.3.0",
    "es6-shim": "^0.35.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "reflect-metadata": "0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "~5.0.0-beta.12",
    "typescript": ">=2.0.2 <=2.0.10",
    "webpack": "^1.13.0",
    "zone.js": "~0.7.2"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "@angular/http": "^2.0.0",
    "rxjs": "^5.0.0"
  }
}
